digraph {
	rankdir=LR
	S0 [label=S0 shape=circle]
	S1 [label=S1 shape=circle]
	S2 [label=S2 shape=circle]
	S3 [label=S3 shape=doublecircle]
	S4 [label=S4 shape=circle]
	start [shape=point]
	start -> S0
	S0 -> S1 [label=b]
	S0 -> S2 [label=a]
	S2 -> S1 [label=b]
	S2 -> S2 [label=a]
	S1 -> S3 [label=a]
	S1 -> S4 [label=b]
	S4 -> S3 [label=a]
	S3 -> S3 [label=a]
}
